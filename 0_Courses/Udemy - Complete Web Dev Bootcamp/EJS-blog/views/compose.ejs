<%- include("partials/header") -%>
<h1>Compose</h1>

<form action="/compose" method="post">

  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="composeTitle">

    <label>Body</label>
    <textarea class="form-control" name="composeBody" rows="6" cols="80"></textarea>
  </div>

  <button class="btn btn-primary" type="submit" name="composeButton">Publish</button>
</form>

<%- include("partials/footer") -%>
